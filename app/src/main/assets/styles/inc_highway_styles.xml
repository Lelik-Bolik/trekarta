<?xml version="1.0" encoding="UTF-8"?>
<rendertheme version="1" xmlns="http://opensciencemap.org/rendertheme">
    <!-- Road styles -->
    <style-line cap="round" id="ford" stroke="#83a3ba" width="0.7" />
    <style-line cap="butt" id="4wd_only" stipple="15" stipple-stroke="#c3a889" stroke="color:transparent"
        width="0.7" />
    <style-line id="trunk" stroke="color:trunk" width="1.3" />
    <style-line cap="butt" id="trunk_link" stroke="color:trunk_link" width="1.0" />
    <style-line id="highway" stroke="color:highway" width="1.2" />
    <style-line id="primary" stroke="color:primary" width="1.0" />
    <style-line id="secondary" stroke="color:secondary" use="primary" width="-0.1" />
    <style-line id="unclassified" stroke="color:unclassified" width="0.8" />
    <style-line id="tertiary" use="unclassified" width="0.1" />
    <style-line id="road-construction" stipple="10" stipple-stroke="color:road-construction-stipple"
        stroke="color:road-construction" width="0.8" />
    <style-line id="residential" stroke="color:residential" use="unclassified" width="-0.1" />
    <style-line id="highway-service" stroke="color:highway-service" width="0.4" />
    <style-line id="pedestrian" stroke="color:pedestrian" use="residential" width="-0.2" />
    <style-area fill="#e0e0e0" id="pedestrian" />
    <style-line cap="butt" fix="true" id="highway:track:back" stroke="#88ffffff" width="2.4" />
    <style-line cap="butt" fix="true" id="highway:track:good" stroke="#99764D" width="1.8" />
    <style-line cap="butt" fix="true" id="highway:track:very_bad" stipple="14" stipple-ratio="0.8" stipple-stroke="#99764D"
        stroke="#00ffffff" width="1.8" />
    <style-line id="highway:track:horrible" stipple="10" stipple-ratio="0.6" use="highway:track:very_bad" />
    <style-line id="highway:track:very_horrible" stipple="8" stipple-ratio="0.4" use="highway:track:very_bad" />
    <style-line id="highway:track:impassable" stroke="#44ff0000" use="highway:track:very_horrible" />
    <style-line cap="butt" fix="true" id="footway:back" stroke="#88ffffff" width="1.4" />
    <style-line cap="butt" fix="true" id="footway" stipple="4" stipple-stroke="#d37c58"
        stroke="#00ffffff" width="1.2" />
    <style-line cap="butt" id="footway:bridge" stroke="#fefefe" use="footway" />

    <style-line fix="true" id="cycleway" stipple-stroke="#4040ee" use="footway" width="0.2" />
    <style-line id="path" stipple="4" stipple-stroke="#837b58" use="footway" width="0.1" />
    <style-line cap="butt" fix="true" id="bridleway" stipple="6" stipple-stroke="#007700"
        stroke="#00ffffff" width="1.5" />
    <style-line cap="butt" fix="true" id="steps" stipple="4" stipple-ratio="0.35" stipple-stroke="#be6253"
        stroke="#aaffffff" width="3.5" />

    <style-line cap="butt" id="tunnel" stipple="20" stipple-stroke="color:tunnel" stroke="color:transparent"
        width="1.3" />
    <style-line id="highway-tunnel" stipple-stroke="color:highway-tunnel" use="tunnel" width="0.5" />
    <style-line id="trunk-tunnel" stipple-stroke="color:trunk-tunnel" use="tunnel" width="0.4" />
    <style-line id="primary-tunnel" stipple-stroke="color:primary-tunnel" use="tunnel" width="0.2" />
    <style-line id="secondary-tunnel" use="primary-tunnel" width="-0.1" />
    <style-line id="unclassified-tunnel" use="tunnel" />
    <style-line id="tertiary-tunnel" use="unclassified-tunnel" width="0.1" />
    <style-line id="residential-tunnel" use="tunnel" width="-0.1" />
    <style-line id="service-tunnel" stipple="10" stipple-stroke="color:service-tunnel" use="tunnel"
        width="-0.9" />
    <style-line id="pedestrian-tunnel" stipple-stroke="color:pedestrian-tunnel" use="tunnel" width="-0.3" />

    <style-text fill="color:road-text" id="road-name" priority="7" size="16"
        stroke="color:road-text-outline" stroke-width="2.0" />
    <style-text style="bold" id="major-road-name" priority="5" use="road-name" />
</rendertheme>